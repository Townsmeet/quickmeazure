{
  "name": "quickmeazure",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "pnpm run db:migrate && NODE_OPTIONS='--no-deprecation' nuxt build",
    "dev": "NODE_OPTIONS='--no-deprecation' nuxt dev",
    "generate": "NODE_OPTIONS='--no-deprecation' nuxt generate",
    "preview": "NODE_OPTIONS='--no-deprecation' nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest -u",
    "test:ci": "vitest run --coverage --run",
    "test:typecheck": "vue-tsc --noEmit --composite false",
    "test:unit": "vitest run --passWithNoTests --dir tests/unit",
    "test:component": "vitest run --passWithNoTests --dir tests/components",
    "test:e2e": "vitest run --passWithNoTests --dir tests/e2e",
    "test:s3": "tsx server/scripts/test-s3-connection.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "tsx server/scripts/run-migrations.ts",
    "db:seed": "tsx server/scripts/seed-plans.ts",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "pnpm run lint && pnpm run typecheck && pnpm run test:ci",
    "quality:fix": "pnpm run lint:fix && pnpm run format",
    "quality:format": "pnpm run format",
    "quality:lint": "pnpm run lint",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue,mjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@libsql/client": "^0.15.15",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^4.0.1",
    "@nuxtjs/seo": "^3.0.3",
    "@pinia/nuxt": "^0.11.0",
    "@sentry/nuxt": "^9.22.0",
    "@vite-pwa/nuxt": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "better-auth": "^1.3.26",
    "cookie-es": "^2.0.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "dayjs-nuxt": "2.1.11",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.42.0",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^4.1.2",
    "nuxt-charts": "^0.2.4",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "sib-api-v3-sdk": "^8.5.0",
    "ts-node": "^10.9.2",
    "userland": "^0.4.3",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-chrts": "^0.2.4",
    "vue-router": "^4.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.41",
    "@iconify-json/simple-icons": "^1.2.35",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/test-utils": "^3.19.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.19",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^13.9.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "happy-dom": "^17.5.6",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "tailwindcss": "4.1.4",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "vitest": "^3.1.4"
  }
}
