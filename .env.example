# Existing environment variables
NUXT_BREVO_API_KEY=your-brevo-api-key
NUXT_PAYSTACK_PUBLIC_KEY=your-paystack-public-key
NUXT_PAYSTACK_SECRET_KEY=your-paystack-secret-key

# Sentry configuration
NUXT_SENTRY_DSN=https://your-public-key@o0.ingest.sentry.io/your-project-id
NUXT_SENTRY_ORG=your-organization-slug
NUXT_SENTRY_PROJECT=your-project-slug
NUXT_SENTRY_AUTH_TOKEN=your-auth-token

# Turso / libsql configuration
# Remote Turso database URL, e.g. libsql://<dbname>-<org>.turso.io
NUXT_TURSO_DATABASE_URL=
# Auth token from Turso (if required by your database)
NUXT_TURSO_AUTH_TOKEN=

# Better Auth
# Public URL of your app used by Better Auth for links in emails, etc.
NUXT_BETTER_AUTH_URL=

# Google OAuth
NUXT_GOOGLE_CLIENT_ID=
NUXT_GOOGLE_CLIENT_SECRET=